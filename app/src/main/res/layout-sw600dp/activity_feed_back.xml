                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dimen15dp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/feedback_secret_editbox"
                        android:layout_width=".1dp"
                        android:layout_height=".1dp"
                        android:focusable="true"
                        android:importantForAutofill="no" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="@dimen/dimen100dp"
                            android:layout_height="@dimen/dimen100dp"
                            android:background="@drawable/circle_image_bg"
                            android:gravity="center">

                            <com.thresholdsoft.apollofeedback.utils.CircularImageView
                                android:id="@+id/customer_image"
                                android:layout_width="@dimen/dimen98dp"
                                android:layout_height="@dimen/dimen98dp"
                                android:contentDescription="@string/app_name"
                                android:scaleType="fitXY"
                                android:src="@drawable/apollo_circle_logo" />

                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="5dp"
                                android:fontFamily="@font/poppins_regular"
                                android:text="@{model.customerofferScreen.customerName != null &amp;&amp; !model.customerofferScreen.customerName.isEmpty ? model.customerofferScreen.customerName : `---`}"
                                android:textColor="@color/color_white"
                                android:textSize="@dimen/dimen25sp" />


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dimen10dp"
                                android:layout_marginLeft="@dimen/dimen10dp"
                                android:layout_marginTop="@dimen/dimen3dp"
                                android:background="@drawable/black_curves_bg"
                                android:paddingStart="@dimen/dimen7dp"
                                android:paddingTop="@dimen/dimen2dp"
                                android:paddingEnd="@dimen/dimen7dp"
                                android:paddingBottom="@dimen/dimen2dp"
                                android:visibility="@{model.customerofferScreen.customerType != null &amp;&amp; !model.customerofferScreen.customerType.isEmpty ? view.VISIBLE : view.GONE}">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/poppins_regular"
                                    android:text="@{model.customerofferScreen.customerType}"
                                    android:textAllCaps="true"
                                    android:textColor="@color/color_white"
                                    android:textSize="@dimen/dimen18sp" />
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>
                    <!--<ImageView
                        android:id="@+id/customer_image"
                        android:layout_width="@dimen/dimen100dp"
                        android:layout_height="@dimen/dimen100dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/apollo_circle_logo" />-->

